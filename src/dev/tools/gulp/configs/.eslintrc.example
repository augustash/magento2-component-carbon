env:
  amd: true
  browser: true
  es6: true
  jquery: true
  node: true
rules:
  brace-style: [2, "stroustrup"]
  camelcase: 1
  comma-dangle: [2, "never"]
  comma-style: [2, "last"]
  curly: [2, "all"]
  default-case: 2
  eol-last: 1
  eqeqeq: [2, "allow-null"]
  func-style: [2, "declaration"]
  global-strict: 0
  guard-for-in: 1
  indent: 0
  keyword-spacing: [1, {"before": true, "after": true}]
  new-cap: 2
  no-alert: 2
  no-caller: 1
  no-console: 2
  no-debugger: 2
  no-empty: 1
  no-extra-semi: 2
  no-floating-decimal: 2
  no-invalid-regexp: 2
  no-nested-ternary: 2
  no-new: 2
  no-obj-calls: 2
  no-shadow: 1
  no-undef: 2
  no-undefined: 2
  no-underscore-dangle: 0
  no-unreachable: 2
  no-unused-vars: [0, {"vars": "local", "args": "after-used"}]
  no-use-before-define: 1
  quotes: [1, "single"]
  radix: 2
  semi: 2
  space-before-blocks: 2
  space-before-function-paren: [2, "never"]
  spaced-comment: [2, "always", { exceptions: ["-"] }]
  strict: [2, "global"]
  valid-jsdoc: [1, { requireReturn: false, prefer: { return: "returns" }}]
  wrap-iife: [2, "inside"]
globals:
  jQuery: false
  $j: true
